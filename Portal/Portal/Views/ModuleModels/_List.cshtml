<section id="module-view-list">
        Click on row for edit
        <table class="table table-striped table-bordered table-condensed">
            <thead>
                <tr>
                    @*<th>Id</th>*@
                    <th>Name</th>
                    <th>Description</th>
                    <th>Gadget Url</th>
                    <th>Thumbnail</th>
                    <th>Is Public</th>
                </tr>
            </thead>

            <tbody data-bind="foreach: modules">
                <tr data-bind="click: $parent.editModule">
                    @*<td data-bind="text: Id"></td>*@
                    <td data-bind="text: Name"></td>
                    <td data-bind="text: Description"></td>
                    <td data-bind="text: GadgetUrl"></td>
                    <td data-bind="text: Thumbnail"></td>
                    <td><input type="checkbox" data-bind="checked: IsPublic" /></td>
                    @*<td><input type="button" class="btn btn-danger btn-xs" value=" [x] delete " data-bind="click: $parent.removeModule" /></td>*@
                </tr>
            </tbody>
        </table>

        <br />

        <input type="button" class="btn bg-blue" id="btnGetModules" value="Refresh" data-bind="click: getModules" />
</section>