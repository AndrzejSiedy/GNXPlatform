  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <add key="Neo4j" value="http://localhost:7474/db/data" />

      <!-- an url for .NET SignalR client -->
      <add key="signalRUrl" value="http://localhost:47342/SignalR" />
      <!-- an URL for JS SignalR client -->
      <add key="signalRHubsUrl" value="http://localhost:47342/SignalR/hubs" />

      <!-- name of SignalR Hub Name -->
      <add key="hubName" value="gnxHub" />
      
      <!-- Add config for Apache Shindig container -->
      <add key="shindigContainerUrl" value="http://localhost:8088/containers/commoncontainer/"/>
      
  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <modules>
      <remove name="FormsAuthenticationModule" />
    </modules>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
