  <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 --></configSections>
  <connectionStrings>
    <!--<add name="DefaultConnection" connectionString="Data Source=(LocalDb)\v11.0;AttachDbFilename=|DataDirectory|\aspnet-Portal-20140613022706.mdf;Initial Catalog=aspnet-Portal-20140613022706;Integrated Security=True" providerName="System.Data.SqlClient" />-->
